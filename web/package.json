{
  "name": "courier-ui",
  "version": "0.1.0",
  "description": "A web ui for the courier pubsub service.",
  "main": "dist/index.html",
  "repository": "https://github.com/davidMcneil/courier",
  "author": "David McNeil",
  "license": "(MIT OR Apache-2.0)",
  "browserslist": "defaults",
  "scripts": {
    "start": "parcel --out-dir=dev index.html",
    "build": "parcel build --detailed-report --no-source-maps --public-url=./ index.html",
    "clean": "rm -rf dist/ && rm -rf dev/",
    "size": "du -sh dist/* && du -sh dist/"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "classnames": "^2.2.6",
    "inferno": "^5.6.1",
    "inferno-router": "^5.6.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "babel-plugin-inferno": "^5.1.0",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.9.7",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
