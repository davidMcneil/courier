{
  "name": "courier-ui",
  "version": "0.1.0",
  "description": "A web ui for the courier pubsub service.",
  "main": "dist/index.html",
  "repository": "https://github.com/davidMcneil/courier",
  "author": "David McNeil",
  "license": "(MIT OR Apache-2.0)",
  "browserslist": "defaults",
  "scripts": {
    "start": "parcel --out-dir=dev index.html",
    "build": "parcel build --detailed-report --no-source-maps --public-url=/ui index.html",
    "clean": "rm -rf dist/ && rm -rf dev/",
    "size": "du -sh dist/* && du -sh dist/"
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "hoist-non-inferno-statics": "^1.1.3",
    "inferno": "^7.0.5",
    "inferno-router": "^7.0.5",
    "inferno-shared": "^7.0.5",
    "path-to-regexp-es6": "1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/classnames": "^2.2.7",
    "babel-plugin-inferno": "^6.0.4",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.11.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.4"
  }
}
